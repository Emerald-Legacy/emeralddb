{
  "name": "fiveringsdb-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:clean && yarn build:server && yarn build:client",
    "build:clean": "rimraf dist",
    "build:client": "cd client && yarn install && yarn build && cd .. && cp -r client/build dist/public",
    "build:server": "tsc",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "migrate:latest": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "start:all:dev": "concurrently \"yarn start:server:dev\" \"cd client && yarn start\"",
    "start:server:dev": "cross-env NODE_ENV=development sh bin/watch.sh",
    "start": "NODE_ENV=production yarn migrate:latest && node dist/index.js",
    "test": "yarn lint && yarn test:unit && yarn build && ava",
    "test:unit": "yarn build:clean && ava",
    "test:unit:watch": "ava --watch"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "volta": {
    "node": "22.11.0"
  },
  "license": "MIT",
  "dependencies": {
    "@5rdb/api": "file:./apiTypings",
    "@discordjs/builders": "^0.11.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "@sentry/node": "^6.2.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-jwt": "^6.0.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.170",
    "@types/node": "^22.18.10",
    "@types/passport": "^1.0.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.5.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "express": "^4.17.1",
    "express-async-router": "^0.1.15",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "fuse.js": "^6.5.3",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.2",
    "knex": "^0.95.2",
    "passport": "^0.4.1",
    "pg": "^8.5.1",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.16",
    "typescript": "^5.9.3",
    "uuid": "^8.3.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1"
  }
}
