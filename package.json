{
  "name": "fiveringsdb-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:clean && yarn build:server && yarn build:client",
    "build:clean": "rimraf dist",
    "build:client": "cd client && yarn install && yarn build && cd .. && cp -r client/build dist/public",
    "build:server": "tsc",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "migrate:latest": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "start:all:dev": "concurrently \"yarn start:server:dev\" \"cd client && yarn start\"",
    "start:server:dev": "cross-env NODE_ENV=development sh bin/watch.sh",
    "start": "NODE_ENV=production yarn migrate:latest && node dist/index.js",
    "test": "yarn lint && yarn test:unit && yarn build && ava",
    "test:unit": "yarn build:clean && ava",
    "test:unit:watch": "ava --watch"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "volta": {
    "node": "22.11.0"
  },
  "license": "MIT",
  "dependencies": {
    "@5rdb/api": "file:./apiTypings",
    "@discordjs/builders": "^1.10.0",
    "@discordjs/rest": "^2.4.0",
    "@sentry/node": "^8.48.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^5.0.3",
    "@types/express-jwt": "^6.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.14.170",
    "@types/node": "^22.18.10",
    "@types/passport": "^1.0.17",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.22.1",
    "@typescript-eslint/parser": "^8.22.1",
    "body-parser": "^1.20.3",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "discord-api-types": "^0.37.100",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^3.0.1",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "express-jwt-authz": "^2.4.1",
    "fuse.js": "^7.0.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "knex": "^3.1.0",
    "passport": "^0.7.0",
    "pg": "^8.16.3",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "typescript": "^5.9.3",
    "uuid": "^11.0.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "ava": "^6.2.0",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2"
  }
}
